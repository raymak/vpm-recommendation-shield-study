{
  "name": "VPN Recommendation Shield Study (bug 1478770)",
  "description": "This study extension recommends a VPN software solution to those users who it deems likely to benefit from it.",
  "version": "0.6",
  "manifest_version": 2,
  "applications": {
    "gecko": {
      "id": "vpn-recommendation-study-1@shield.mozilla.org",
      "strict_min_version": "61.0a1"
    }
  },
  "experiment_apis": {
    "vpnRecommender": {
      "schema": "./privileged/vpnRecommender/schema.json",
      "parent": {
        "scopes": ["addon_parent"],
        "script": "./privileged/vpnRecommender/api.js",
        "paths": [["experiments", "vpnRecommender"]]
      }
    },
    "study": {
      "schema": "./privileged/study/schema.json",
      "parent": {
        "scopes": ["addon_parent"],
        "script": "./privileged/study/api.js",
        "paths": [["study"]]
      }
    },
    "testingOverrides": {
      "schema": "./privileged/testingOverrides/schema.json",
      "parent": {
        "scopes": ["addon_parent"],
        "script": "./privileged/testingOverrides/api.js",
        "paths": [["testingOverrides"]]
      }
    }
  },
  "permissions": ["management", "alarms", "storage"],
  "background": {
    "scripts": ["studySetup.js", "background.js", "feature.js"]
  },
  "hidden": true,
  "icons": {
    "48": "icon.svg",
    "96": "icon.svg"
  }
}
