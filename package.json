    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-mozilla": "^0.16.1",
    "eslint-plugin-no-unsanitized": "^3.0.2",
    "shield-studies-addon-utils": "^5.0.3",
    "fx-runner": "^1.0.9",
    "geckodriver": "^1.12.2",
    "get-firefox": "^2.0.0",
    "mocha": "^5.2.0",
